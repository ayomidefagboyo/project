# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
# Backend-only secrets such as STRIPE_SECRET_KEY and STRIPE_WEBHOOK_SECRET
# must stay in backend environment variables, never in frontend env files.

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# PostHog Analytics Configuration
VITE_POSTHOG_API_KEY=your_posthog_api_key_here
VITE_POSTHOG_HOST=https://app.posthog.com

# Google Cloud Vision API for OCR
VITE_GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Demo-specific OCR API Key (with restrictions)
VITE_GOOGLE_VISION_DEMO_API_KEY=your_restricted_demo_api_key_here

# OCR Configuration
VITE_ENABLE_OCR=true
VITE_OCR_ENABLED=true

# Other Environment Variables
VITE_APP_URL=http://localhost:5173
NODE_ENV=development
